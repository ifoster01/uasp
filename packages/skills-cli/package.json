{
  "name": "uasp-skills",
  "version": "0.1.6",
  "description": "CLI tool for managing UASP agent skills",
  "type": "module",
  "bin": {
    "uasp-skills": "./bin/uasp-skills.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node bin/uasp-skills.js",
    "lint": "eslint src",
    "test": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "benchmark": "node --expose-gc dist/benchmark/index.js",
    "benchmark:dev": "tsx --expose-gc src/benchmark/index.ts",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "fs-extra": "^11.2.0",
    "node-fetch": "^3.3.0",
    "ora": "^8.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^20.19.30",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.0.0",
    "gray-matter": "^4.0.3",
    "tsx": "^4.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "uasp",
    "agent",
    "skills",
    "cli",
    "llm",
    "ai"
  ],
  "author": "ifoster01",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ifoster01/uasp"
  },
  "homepage": "https://github.com/ifoster01/uasp/tree/main/packages/skills-cli",
  "bugs": {
    "url": "https://github.com/ifoster01/uasp/issues"
  }
}
